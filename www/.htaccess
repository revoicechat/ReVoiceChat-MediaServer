RewriteEngine On
RewriteCond %{REQUEST_URI} !\.php$

# Attachements
RewriteRule attachements/([0-9A-Za-z-]+)$ index.php?attachements=$1

# Profiles
RewriteRule profiles/([0-9A-Za-z-]+)$ index.php?profiles=$1

# Emojis global
RewriteRule emojis/global/upload emojis.php?global&upload
RewriteRule emojis/global/all emojis.php?global&all
RewriteRule emojis/global/([0-9A-Za-z-.]*)$ emojis.php?global&emojis=$1

# Emojis server
RewriteRule emojis/server/([0-9A-Za-z-.]*)$/upload emojis.php?server=$1&upload
RewriteRule emojis/server/([0-9A-Za-z-.]*)$/all emojis.php?server=$1&all
RewriteRule emojis/server/([0-9A-Za-z-.]*)/([0-9A-Za-z-.]*)$ emojis.php?server=$1&emojis=$2
RewriteEngine On
RewriteCond %{REQUEST_URI} !\.php$

# Attachements
RewriteRule ^media/attachments/([0-9A-Za-z-]+)$ index.php?attachment=$1 [L]

# Profiles
RewriteRule ^media/profiles/([0-9A-Za-z-]+)$ index.php?profiles=$1 [L]

# Emojis Any
RewriteRule ^media/emojis/([0-9A-Za-z-.]*)$ emojis.php?any&id=$1 [L]

# Emojis global
RewriteRule ^media/emojis/global/all emojis.php?global&all [L]
RewriteRule ^media/emojis/global/([0-9A-Za-z-.]*)$ emojis.php?global&id=$1 [L]

# Other
RewriteRule ^media/([0-9A-Za-z-.]*)$ index.php?$1 [L]
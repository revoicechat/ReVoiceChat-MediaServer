RewriteEngine On
RewriteCond %{REQUEST_URI} !\.php$

# Attachements
RewriteRule attachements/([0-9A-Za-z-]+)$ index.php?attachements=$1

# Profiles
RewriteRule profiles/([0-9A-Za-z-]+)$ index.php?profiles=$1

# Emojis global
RewriteRule emojis/global/all index.php?emojis-global=all
RewriteRule emojis/global/([0-9A-Za-z-.]+)$ index.php?emojis-global=$1

RewriteRule emojis/bulk index.php?emojis=bulk
RewriteRule emojis/([0-9A-Za-z-.]+)$ index.php?emojis=$1
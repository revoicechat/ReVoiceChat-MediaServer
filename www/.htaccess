RewriteEngine On
RewriteCond %{REQUEST_URI} !\.php$

# Attachements
RewriteRule attachments/([0-9A-Za-z-]+)$ index.php?attachment=$1 [L]

# Profiles
RewriteRule profiles/([0-9A-Za-z-]+)$ index.php?profiles=$1 [L]

# Emojis Any
RewriteRule emojis/([0-9A-Za-z-.]*)$ emojis.php?any&id=$1 [L]

# Emojis global
RewriteRule emojis/global/all emojis.php?global&all [L]
RewriteRule emojis/global/([0-9A-Za-z-.]*)$ emojis.php?global&id=$1 [L]